{
  "version": 2,
  "builds": [
    {
      "src": "FrontEnd/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "FrontEnd/index.html" },
    { "src": "/app.html", "dest": "FrontEnd/app.html" },
    { "src": "/otp.html", "dest": "FrontEnd/otp.html" },
    { "src": "/style.css", "dest": "FrontEnd/style.css" },
    { "src": "/script.js", "dest": "FrontEnd/script.js" },
    { "src": "/editor.html", "dest": "FrontEnd/editor.html" },
    { "src": "/editor.js", "dest": "FrontEnd/editor.js" },
    { "src": "/editor.css", "dest": "FrontEnd/editor.css" }
  ]
}
