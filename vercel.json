{
  "version": 2,
  "builds": [
    {
      "src": "FrontEnd/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://hivemind-backend-9u2f.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/FrontEnd/$1"
    }
  ]}