{
  "version": 2,
  "builds": [
    {
      "src": "FrontEnd/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/FrontEnd/index.html"
    },
    {
      "source": "/app.html",
      "destination": "/FrontEnd/app.html"
    },
    {
      "source": "/otp.html",
      "destination": "/FrontEnd/otp.html"
    },
    {
      "source": "/style.css",
      "destination": "/FrontEnd/style.css"
    }
  ]
}
