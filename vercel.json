{
  "version": 2,
  "builds": [
    {
      "src": "FrontEnd/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/FrontEnd/index.html"
    },
    {
      "src": "/app.html",
      "dest": "/FrontEnd/app.html"
    },
    {
      "src": "/otp.html",
      "dest": "/FrontEnd/otp.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/FrontEnd/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/FrontEnd/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 